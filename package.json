{
  "name": "reactStarter",
  "version": "1.0.0",
  "private": true,
  "description": "React Typescript",
  "scripts": {
    "start": "node tasks/start",
    "test": "jest",
    "build": "gulp build",
    "build-prod": "npm install && gulp build:prod",
    "typings": "rm -rf typings && typings install",
    "postinstall": "npm run typings",
    "cleaninstall": "rm -rf node_modules && npm install",
    "watch-test": "jest --watch",
    "lint:ts": "node tasks/tslint"
  },
  "author": "Jeff Noble",
  "license": "",
  "devDependencies": {
    "@types/sinon": "^2.3.2",
    "awesome-typescript-loader": "^3.2.3",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.9",
    "clone": "^1.0.2",
    "css-loader": "^0.23.1",
    "currency-formatter": "^1.0.2",
    "enzyme": "http://sf-nuget.citrite.net:81/npm/sf-npm/enzyme/-/enzyme-2.8.2.tgz",
    "es6-promise": "^4.0.5",
    "es6-shim": "^0.35.1",
    "extract-text-webpack-plugin": "^0.3.8",
    "fbjs-scripts": "^0.6.0",
    "file-loader": "^0.8.5",
    "fs": "0.0.2",
    "gulp": "^3.9.1",
    "gulp-add-src": "^0.2.0",
    "gulp-istanbul": "^0.10.4",
    "gulp-mocha": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^6.1.2",
    "gulp-typescript": "^2.13.4",
    "gulp-util": "^3.0.7",
    "jest-cli": "^18.1.0",
    "jest-junit": "^1.5.1",
    "jquery": "^2.2.2",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.2",
    "minimatch": "^3.0.0",
    "mock-local-storage": "^1.0.4",
    "moment": "^2.14.1",
    "path": "^0.12.7",
    "progress-bar-webpack-plugin": "^1.10.0",
    "rcfinder": "^0.1.9",
    "react-addons-test-utils": "^15.0.1",
    "sinon": "http://sf-nuget.citrite.net:81/npm/sf-npm/sinon/-/sinon-1.17.7.tgz",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.2",
    "through2": "^2.0.1",
    "ts-loader": "^0.8.2",
    "ts-node": "^2.1.0",
    "tslint": "^4.3.1",
    "tslint-eslint-rules": "^3.2.3",
    "tslint-microsoft-contrib": "^4.0.0",
    "tslint-misc-rules": "^2.9.0",
    "typings": "^1.3.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "url-loader": "^0.5.9",
    "validator": "^5.3.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^1.16.5",
    "webpack-md5-hash": "0.0.5"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.ts"
    ],
    "coverageReporters": [
      "text-summary",
      "html",
      "clover"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "notify": true,
    "setupTestFrameworkScriptFile": "<rootDir>/tasks/jestEnvironment.js",
    "testResultsProcessor": "<rootDir>/node_modules/jest-junit",
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/tasks/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "timers": "fake",
    "unmockedModulePathPatterns": [
      "enzyme",
      "react",
      "aphrodite",
      "moment",
      "ts-jest"
    ]
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "aphrodite": "^1.1.0",
    "axios": "^0.16.2",
    "bootstrap": "^3.3.7",
    "bootstrap-webpack": "0.0.6",
    "change-case": "^3.0.1",
    "crypto": "0.0.3",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.3.8",
    "imports-loader": "^0.6.3",
    "jquery-validation": "^1.15.1",
    "jqueryui": "^1.11.1",
    "libphonenumber-js": "^0.3.14",
    "moment-timezone": "^0.5.13",
    "qs": "^6.4.0",
    "react": "^15.4.2",
    "react-bootstrap": "https://registry.npmjs.org/react-bootstrap/-/react-bootstrap-0.30.3.tgz",
    "react-bootstrap-modal": "^3.0.0",
    "react-datepicker": "^0.37.0",
    "react-dom": "^15.4.2",
    "react-modal": "^1.7.1",
    "react-radio-group": "^3.0.1",
    "react-router": "^2.4.1",
    "react-select": "^1.0.0-rc.2",
    "react-validation": "^2.10.9",
    "readable-stream": "^2.2.2",
    "sf-api": "http://sf-nuget.citrite.net:81/npm/sf-npm/sf-api/-/sf-api-4.227.0.tgz",
    "toastr": "^2.1.2",
    "tslint": "^4.5.1",
    "typescript": "2.2.2",
    "validator": "^5.7.0"
  }
}
